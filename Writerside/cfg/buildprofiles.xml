<?xml version="1.0" encoding="UTF-8"?>

<buildprofiles>
    <variables>
        <header-logo>logo.svg</header-logo>
        <!--启用Github编辑-->
        <enable-browser-edits>true</enable-browser-edits>
        <browser-edits-url>https://github.com/T-miracle/kotlin-docs-cn/edit/main/Writerside/</browser-edits-url>
        <!--主题色-->
        <primary-color>purple</primary-color>
        <kotlin-latest-url>%kotlinLatestUrl%</kotlin-latest-url>
        <!--首页地址-->
        <product-web-url>https://blog.namichong.com/translation-docs/kotlin-docs/</product-web-url>
        <generate-only-default-anchors>true</generate-only-default-anchors>
        <web-root>https://blog.namichong.com/translation-docs/kotlin-docs/</web-root>
        <og-image>https://kotlinlang.org/assets/images/open-graph/docs.png</og-image>
        <og-twitter>@kotlin</og-twitter>
        <web-community-path>Kotlin Community:https://kotlinlang.org/community/</web-community-path>
        <download-page/>
        <relative-url-to-absolute-prefix>https://github.com/Kotlin/kotlinx.coroutines/blob/master/</relative-url-to-absolute-prefix>
        <feedback-support>https://kotl.in/issue</feedback-support>
        <feedback-url>https://data.services.jetbrains.com/feedback</feedback-url>
        <webmaster>doc-feedback@kotlinlang.org</webmaster>
        <sideblocks>true</sideblocks>
        <!--<config-algolia-api-key>1bfad5fdbae302b33d844ed1b43ec4d5</config-algolia-api-key>
        <config-algolia-id>7961PKYRXV</config-algolia-id>
        <config-algolia-index>prod_KOTLINLANG_WEBHELP</config-algolia-index>-->
        <noindex-content status="deprecated">false</noindex-content>
        <noindex-content status="eap">false</noindex-content>
        <noindex-content status="release">false</noindex-content>
        <config-search-scopes-provider>https://www.jetbrains.com/search/json/</config-search-scopes-provider>
        <generate-canonicals>true</generate-canonicals>
        <!--谷歌分析-->
        <analytics-head-script-file>google-analytics-code.html</analytics-head-script-file>
        <analytics-body-html-file>google-analytics-noscript.html</analytics-body-html-file>
    </variables>
    <build-profile product="kotlin">
        <variables>
            <noindex-content>false</noindex-content>
        </variables>
    </build-profile>

    <footer>
        <link href="https://github.com/JetBrains/kotlin">Github 源码</link>
        <link href="https://twitter.com/kotlin">Twitter</link>
        <link href="https://blog.namichong.com/">译者站点</link>
    </footer>
</buildprofiles>
